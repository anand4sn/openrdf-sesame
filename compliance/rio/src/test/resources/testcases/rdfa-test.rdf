<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:rnd="http://random.ioctl.org/#"
  xmlns:xhtml="http://www.w3.org/1999/xhtml#"
  xmlns:vocab="http://www.w3.org/1999/xhtml/vocab#">

<rdf:Description rdf:about="">
  <xhtml:title>RDFa Test File</xhtml:title>
  <vocab:copyright>2009</vocab:copyright>
  <vocab:start rdf:resource="http://random.ioctl.org/"/>
  <xhtml:style>div[id], div[about] {
margin: 1em;
}

span[property="rdfs:label"] {
display: block;
font-size: 2em;
font-weight: bold;
margin: 0px 0.67em;
}

span[property="rdfs:comment"] {
  display: block;
  margin: 1em 40px;
}

a[rel] {
  display: block;
}

a[rel="rdfs:isDefinedBy"] {
  display: none;
}

a[rel="rdfs:subClassOf"]:before {
  content: "subClass of: "
}

a[rel="rdfs:domain"]:before {
  content: "domain: "
}

a[rel="rdfs:range"]:before {
  content: "range: "
}

a[ral="rdfs:subPropertyOf"]:before {
  content: "subProperty of"
}

a[rel="rdfs:seeAlso"]:before {
  content: "see also: "
}</xhtml:style>
</rdf:Description>

<rdf:Description rdf:ID="bar">
  <rnd:label>Some label</rnd:label>
  <rnd:subNode>
    <rnd:Node>
       <rdfs:label>sub node</rdfs:label>
       <rnd:relatedTo rdf:resource="#foo"/>
    </rnd:Node>
  </rnd:subNode>
</rdf:Description>

<!-- list comment -->
<xhtml:ul rdf:ID="list">
  <xhtml:li>first item</xhtml:li>
  <xhtml:li>
    <xhtml:ul>
      <xhtml:li>first item</xhtml:li>
      <xhtml:li>second item</xhtml:li>
      <xhtml:li>third item</xhtml:li>
    </xhtml:ul>
  </xhtml:li>
  <xhtml:li>third item</xhtml:li>
</xhtml:ul>

<rdf:Description rdf:ID="foo">
  <rdfs:label>Foo label</rdfs:label>
  <rnd:shortFor rdf:resource="#foot"/>
</rdf:Description>

<rdf:Description rdf:ID="foot">
  <rdfs:label>Foot label</rdfs:label>
</rdf:Description>

<rnd:Type rdf:ID="type"/>

</rdf:RDF>

